        <p rend="center" xml:id="K_6069_p8">
                    <lb xml:id="p4-l4"/>
          <choice>
                        <abbr>Dr</abbr>
                        <expan>Doktor / Doctor</expan>
                    </choice> A Escher</p>
      </div>
      <div type="dateline">
        <p rend="left" xml:id="K_6069_p9">
                    <lb xml:id="p4-l5"/>
          <placeName type="place" key="Zürich (Stadt)">Z.</placeName>
          <date when="1867-02-14">14 <choice>
                            <abbr>Febr<hi rend="sup">r</hi> </abbr>
                            <expan>Februar</expan>
                        </choice> 67</date>
        </p>
      </div>
    </back>
