          <placeName type="place" key="Bern (Stadt)">Bern</placeName>
          <lb xml:id="p3-l14"/>
          <date when="1867-09-07">7. <choice>
                            <abbr>Sept.</abbr>
                            <expan>September</expan>
                        </choice> 1867</date>.</p>
      </div>
