        <p rend="left" xml:id="K_5172_p20">
                    <lb xml:id="p4-l16"/>
          <hi>
            <placeName type="place" key="Florenz (I)">Florenz</placeName>
                    </hi>, <date when="1866-01-26">26
                            <hi>Januar</hi> 1866</date>.</p>
      </div>
