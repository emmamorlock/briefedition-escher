  <text>
    <front>
      <div type="printedheader">
        <p rend="left" xml:id="K_6521_p1">
                    <lb xml:id="p1-l1"/>
          <fw>BANK<lb xml:id="p1-l2"/>IN<lb xml:id="p1-l3"/>
