        <p rend="left" xml:id="K_3998_p7">
                    <lb xml:id="p2-l15"/>
          <placeName type="place" key="Luzern (Stadt)">
                        <hi>Luzern</hi>
                    </placeName> d.
					<date when="1866-12-24">24 <choice>
                            <abbr>
                            <hi>Dec.</hi> </abbr>
                            <expan>December</expan>
                        </choice> 1866</date> </p>
      </div>
    </back>
