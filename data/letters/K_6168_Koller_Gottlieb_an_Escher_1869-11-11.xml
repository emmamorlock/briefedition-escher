          <placeName type="place" key="Basel-Stadt">
                        <hi>Basel</hi>
                    </placeName> den <date when="1869-11-11">11 <choice>
                            <abbr>
                                <hi>Nov</hi>.</abbr>
                            <expan>November</expan>
                        </choice> 1869</date> </p>
      </div>
      <div type="address">
        <p rend="center" xml:id="K_6168_p3">
                    <lb xml:id="p1-l4"/> <choice>
                        <abbr>
                            <hi>Tit</hi>.</abbr>
